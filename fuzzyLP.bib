@article{Cabrera20141,
title = "On residuation in multilattices: Filters, congruences, and homomorphisms ",
journal = "Fuzzy Sets and Systems ",
volume = "234",
number = "0",
pages = "1 - 21",
year = "2014",
issn = "0165-0114",
doi = "http://dx.doi.org/10.1016/j.fss.2013.04.002",
url = "http://www.sciencedirect.com/science/article/pii/S0165011413001498",
author = "I.P. Cabrera and P. Cordero and G. Gutiérrez and J. Mart\'i­nez and M. Ojeda-Aciego",
keywords = "Hyperalgebra",
keywords = "Pocrim",
keywords = "Multilattices",
keywords = "Residuation ",
abstract = "Abstract Continuing with our general study of algebraic hyperstructures, we focus on the residuated operation in the framework of multilattices. Firstly, we recall the existing relation between filters, homomorphisms and congruences in the framework of multilattices; then, introduce the notion of residuated multilattice and further study the notion of filter, which has to be suitably modified so that the results in the first section are conveniently preserved also in the residuated case. "
}

@article{Cabrera201231,
title = "Finitary coalgebraic multisemilattices and multilattices ",
journal = "Applied Mathematics and Computation ",
volume = "219",
number = "1",
pages = "31 - 44",
year = "2012",
note = "Towards a Computational Interpretation of Physical TheoriesTowards a Computational Interpretation of Physical Theories ",
issn = "0096-3003",
doi = "http://dx.doi.org/10.1016/j.amc.2011.10.081",
url = "http://www.sciencedirect.com/science/article/pii/S0096300311013270",
author = "I.P. Cabrera and P. Cordero and G. Gutiérrez and J. Martínez and M. Ojeda-Aciego",
keywords = "Lattices",
keywords = "Coalgebras",
keywords = "Non-determinism",
keywords = "Multilattice ",
abstract = "In this paper we continue the coalgebraization of the structure of multilattice. Specifically, we introduce a coalgebraic characterization of the notion of finitary multi (semi) lattice, a generalization of that of semilattice which arises naturally in several areas of computer science and provides the possibility of handling non-determinism. "
}




@article{Cabrera2010,
title = "A coalgebraic approach to non-determinism: Applications to multilattices ",
journal = "Information Sciences ",
volume = "180",
number = "22",
pages = "4323 - 4335",
year = "2010",
note = "",
issn = "0020-0255",
doi = "http://dx.doi.org/10.1016/j.ins.2010.07.002",
url = "http://www.sciencedirect.com/science/article/pii/S0020025510003208",
author = "I.P. Cabrera and P. Cordero and G. Gutiérrez and J. Martínez and M. Ojeda-Aciego",
keywords = "Lattices",
keywords = "Coalgebras",
keywords = "Non-determinism",
keywords = "Multilattices ",
abstract = "Multilattices are a suitable generalization of lattices which enables to accommodate the formalization of non-deterministic computation; specifically, the algebraic characterization for multilattices provides a formal framework to develop tools in several fields of computer science. On the other hand, the usefulness of coalgebra theory has been increasing in the recent years, and its importance is undeniable. In this paper, somehow mimicking the use of universal algebra, we define a new kind of coalgebras (the ND-coalgebras) that allows to formalize non-determinism, and show that several concepts, widely used in computer science are, indeed, ND-coalgebras. Within this formal context, we study a minimal set of properties which provides a coalgebraic definition of multilattices. "
}




