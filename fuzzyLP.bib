@article{ZHANG202178,
title = {Constructions of uni-nullnorms and null-uninorms on a bounded lattice},
journal = {Fuzzy Sets and Systems},
volume = {403},
pages = {78-87},
year = {2021},
note = {Aggregation Operators},
issn = {0165-0114},
doi = {https://doi.org/10.1016/j.fss.2019.10.010},
url = {https://www.sciencedirect.com/science/article/pii/S0165011419304816},
author = {Hua-Peng Zhang and Yao Ouyang and Bernard {De Baets}},
keywords = {Lattice, Triangular norm, Triangular conorm, Uninorm, Uni-nullnorm, Null-uninorm},
abstract = {Recently, Wang et al. proposed the notion of a uni-nullnorm on a bounded lattice and provided two construction methods. In this paper, we point out that the first construction method is invalid since it does not always lead to a uni-nullnorm. Then we present a method to construct a uni-nullnorm on a bounded lattice via a disjunctive uninorm and a triangular norm. Making use of existing construction methods for uninorms on a bounded lattice, we derive several construction methods for uni-nullnorms, including the second construction method by Wang et al. Dually, we present a method to construct a null-uninorm on a bounded lattice via a conjunctive uninorm and a triangular conorm, as well as several derived construction methods for null-uninorms.}
}

@article{KARACAL201533,
title = {Uninorms on bounded lattices},
journal = {Fuzzy Sets and Systems},
volume = {261},
pages = {33-43},
year = {2015},
note = {Theme: Aggregation operators},
issn = {0165-0114},
doi = {https://doi.org/10.1016/j.fss.2014.05.001},
url = {https://www.sciencedirect.com/science/article/pii/S0165011414002164},
author = {Funda Karaçal and Radko Mesiar},
keywords = {Uninorm, Neutral element, Bounded lattice},
abstract = {We study uninorms on bounded lattices. We show the existence of uninorms with the neutral element which is different from the top element and the bottom element using the fact that the t-norms and t-conorms on arbitrary lattice L always exist. As a by-product we give a method of constructing uninorms. Using this method, we obtain also the smallest uninorm and the greatest uninorm with a given neutral element e.}
}

@article{cayli2019,
title = "New methods to construct uninorms on bounded lattices",
journal = "International Journal of Approximate Reasoning",
volume = "115",
pages = "254 - 264",
year = "2019",
issn = "0888-613X",
doi = "https://doi.org/10.1016/j.ijar.2019.10.006",
url = "http://www.sciencedirect.com/science/article/pii/S0888613X19302658",
author = "G{\"u}l Deniz {\c Ca}yl\i",
keywords = "Bounded lattice, Uninorm, Idempotent uninorm, Neutral element, t-Norm, t-Conorm",
abstract = "In this paper, we study idempotent uninorms on bounded lattices. We demonstrate that there need not always exist an idempotent uninorm on a bounded lattice L having the neutral element e?L\{0,1} which differs from the least and the greatest idempotent uninorms. In addition, we propose two new methods to obtain uninorms on bounded lattices with a neutral element, where some sufficient and necessary conditions on the underlying t-norms and t-conorms are required. In particular, two new types of idempotent uninorms on bounded lattices are obtained. We also give an illustrative example for clarity."
}


